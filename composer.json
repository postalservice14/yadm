{
    "name": "postalservice14/yadm",
    "type": "library",
    "description": "Yadm is a MongoDB ODM. Schema less. Fast object hydration and persistence.",
    "keywords": [
        "model",
        "mongodb"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Kotlyar",
            "email": "kotlyar.maksim@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.0|^7.0",
        "mongodb/mongodb": "^1.0.0-beta2@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "scrutinizer/ocular": "^1.1",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Makasim\\Yadm\\": "src"
        },
        "files": [
            "src/functions_include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Makasim\\Yadm\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
